#include<stdio.h>
//#include<conio.h>
#include<stdlib.h>
void bubble(int[],int);
void main()
{
  int a[1000],n,i;
  printf("Enter the number of elements you want to enter");
  scanf("%d",&n);
  for(i=0;i<n;i++)
  {
    int r = (rand() % 1000) + 1;
    a[i]=r;
  }
  for(i=0;i<n;i++)
  {
    printf("%d\n",a[i]);
  }
  bubble(a,n);
  printf("\n Data After Bubble Sort\n");
  for(i=0;i<n;i++)
  {
    printf("%d\t",a[i]);
  }
}
void bubble(int a[],int n)
{
  int i,j,temp;
  for(i=0;i<n;i++)
  {
    for(j=0;j<n-i-1;j++)
    {
      if(a[j]>a[j+1])
      {
        temp=a[j];
        a[j]=a[j+1];
        a[j+1]=temp;
      }
    }
  }
}